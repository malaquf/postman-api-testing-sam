{
    "name": "postman-api-testing-sam",
    "description": "Severless application model for api testing using postman collections",
    "version": "0.0.1",
    "private": true,
    "devDependencies": {
        "@types/node": "^20.6.2",
        "typescript": "^5.2.2",
        "jest": "^29.2.1"
    },
    "scripts": {
        "build": "node_modules/typescript/bin/tsc",
        "watch": "node_modules/typescript/bin/tsc -w --preserveWatchOutput",
        "test": "jest"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js",
            "ts"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.414.0",
        "aws-sdk": "^2.1460.0",
        "lambda-log": "^3.1.0",
        "newman": "^6.0.0",
        "node-cache": "^5.1.2"
    }
}
